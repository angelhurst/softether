[Unit]
Description=Gunicorn instance to serve myproject
After=network.target

[Service]
User=yourusername
Group=www-data
WorkingDirectory=/path/to/your/project
ExecStart=/path/to/venv/bin/gunicorn --config /path/to/your/project/gunicorn_config.py myproject.wsgi:application

[Install]
WantedBy=multi-user.target
